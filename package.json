{
  "name": "lacona-define",
  "version": "1.0.0",
  "description": "Lacona Command for defining dictionary words",
  "lacona": {
    "title": "Dictionary"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laconalabs/lacona-define.git"
  },
  "keywords": [
    "lacona",
    "define"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laconalabs/lacona-define/issues"
  },
  "homepage": "https://github.com/laconalabs/lacona-define",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "elliptical": "^2.0.0",
    "lacona-api": "^1.4.1",
    "lacona-phrases": "^1.1.0",
    "lodash": "^4.16.4",
    "rxjs": "^5.0.0-rc.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-function-bind"
    ]
  },
  "main": "index.js",
  "author": "Brandon Horst <brandonhorst@gmail.com> (http://brandonhorst.me/)"
}
